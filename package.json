{
	"name": "seat-selection-demo",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"clean": "find . -name node_modules -o -name .next -o -name .turbo -o -name dist -type d -prune | xargs rm -rf",
		"format": "biome check --apply . && biome format --write .",
		"dev": "next dev --turbo",
		"lint": "next lint",
		"start": "next start",
		"build": "next build",
		"clean:lock": "rm -f pnpm-lock.yaml",
		"clean:resync": "pnpm clean:lock && pnpm install",
		"bump:latest": "pnpm up -i -latest"
	},
	"dependencies": {
		"@radix-ui/react-dropdown-menu": "^2.0.6",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-scroll-area": "^1.0.5",
		"@radix-ui/react-separator": "^1.0.3",
		"@radix-ui/react-slot": "^1.0.2",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"lucide-react": "^0.367.0",
		"next": "14.2.0",
		"next-themes": "^0.3.0",
		"react": "^18",
		"react-dom": "^18",
		"sharp": "^0.33.3",
		"sonner": "^1.4.41",
		"tailwind-merge": "^2.2.2",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@biomejs/biome": "1.6.4",
		"@types/node": "^20",
		"@types/react": "^18.2.77",
		"@types/react-dom": "^18.2.25",
		"@vercel/style-guide": "^6.0.0",
		"autoprefixer": "^10.0.1",
		"eslint": "^8",
		"eslint-config-next": "14.2.0",
		"eslint-plugin-only-warn": "^1.1.0",
		"postcss": "^8",
		"tailwindcss": "^3.3.0",
		"typescript": "^5"
	},
	"engines": {
		"node": ">=20.x",
		"npm": ">=7.0.0",
		"pnpm": ">=8.0.0"
	},
	"packageManager": "pnpm@8.15.6"
}
