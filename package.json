{
	"name": "seat-selection-demo",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"prisma": {
		"seed": "node prisma/seed.js"
	},
	"scripts": {
		"postinstall": "pnpm db:generate",
		"dev": "next dev --port 3011",
		"start": "next start",
		"build": "pnpm db:deploy && next build",
		"lint": "next lint",
		"typecheck": "tsc --noEmit",
		"format": "biome check --apply . && biome format --write .",
		"db:generate": "prisma generate",
		"db:push": "prisma db push --skip-generate --accept-data-loss",
		"db:seed": "prisma db seed",
		"db:migrate": "prisma migrate dev",
		"db:deploy": "prisma migrate deploy",
		"db:studio": "prisma studio --port 5556",
		"clean": "find . -name node_modules -o -name .next -o -name .turbo -o -name dist -type d -prune | xargs rm -rf",
		"clean:lock": "rm -f pnpm-lock.yaml",
		"clean:resync": "pnpm clean:lock && pnpm install",
		"bump:latest": "pnpm up -i -latest"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^2.1.0",
		"@hookform/resolvers": "^3.4.2",
		"@prisma/client": "^5.10.2",
