{
	"name": "seat-selection-demo",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"postinstall": "pnpm db:generate",

		"dev": "next dev --turbo",
		"start": "next start",
		"build": "next build",

		"lint": "next lint",
		"typecheck": "tsc --noEmit",
		"format": "biome check --apply . && biome format --write .",

		"db:generate": "prisma generate",
		"db:push": "prisma db push --skip-generate --accept-data-loss",
		"db:migrate": "prisma migrate dev",
		"db:deploy": "prisma migrate deploy",
		"db:studio": "prisma studio --port 5556",

		"clean": "find . -name node_modules -o -name .next -o -name .turbo -o -name dist -type d -prune | xargs rm -rf",
		"clean:lock": "rm -f pnpm-lock.yaml",
		"clean:resync": "pnpm clean:lock && pnpm install",

		"bump:latest": "pnpm up -i -latest"
	},
	"dependencies": {
		"@auth/prisma-adapter": "^1.4.0",
		"@prisma/client": "^5.10.2",
		"@radix-ui/react-collapsible": "^1.0.3",
		"@radix-ui/react-dropdown-menu": "^2.0.6",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-scroll-area": "^1.0.5",
		"@radix-ui/react-separator": "^1.0.3",
		"@radix-ui/react-slot": "^1.0.2",
		"@radix-ui/react-tooltip": "^1.0.7",
		"@t3-oss/env-nextjs": "^0.10.1",
		"@tanstack/react-query": "^5.25.0",
		"@trpc/client": "next",
		"@trpc/react-query": "next",
		"@trpc/server": "next",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"lucide-react": "^0.378.0",
		"next": "14.2.3",
		"next-auth": "^4.24.6",
		"next-themes": "^0.3.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"server-only": "^0.0.1",
		"sharp": "^0.33.4",
		"sonner": "^1.4.41",
		"superjson": "^2.2.1",
		"tailwind-merge": "^2.3.0",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.23.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.7.3",
		"@types/eslint": "^8.56.2",
		"@types/node": "^20.12.12",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.9.0",
		"@typescript-eslint/parser": "^7.9.0",
		"autoprefixer": "^10.0.1",
		"eslint": "^8",
		"eslint-config-next": "14.2.3",
		"eslint-plugin-only-warn": "^1.1.0",
		"postcss": "^8",
		"prisma": "^5.10.2",
		"tailwindcss": "^3.3.0",
		"typescript": "^5"
	},
	"engines": {
		"node": ">=20.x",
		"npm": ">=7.0.0",
		"pnpm": ">=8.0.0"
	},
	"packageManager": "pnpm@8.15.5"
}
